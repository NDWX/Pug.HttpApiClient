<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<AssemblyName>Pug.HttpApiClient.Common</AssemblyName>
		<RootNamespace>Pug.HttpApiClient</RootNamespace>
		<Title>Pug.HttpApiClient.Common</Title>
		<Authors>Andrian</Authors>
		<Description>HTTP API client common library</Description>
		<AssemblyVersion>0.3.1</AssemblyVersion>
		<FileVersion>0.3.1</FileVersion>
		<RepositoryUrl>https://github.com/NDWX/Pug.HttpApiClient.git</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>0.3.1</PackageVersion>
		<PackageId>Pug.HttpApiClient.Common</PackageId>
		<PackageTags>Pug HTTP API Client Common Types Library</PackageTags>
		<PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageReleaseNotes>
			Design corrections.
		</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Http" Version="2.1.1" />
	</ItemGroup>

</Project>
